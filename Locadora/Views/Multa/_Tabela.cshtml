@model IEnumerable<Multa>

<table id="example" class="table table-responsive table-striped table-bordered display" style="width:100%">
    <thead>
        <tr>
            <th>CPF</th>
            <th>Infrator</th>
            <th>Valor</th>
            <th>Data de Vencimento</th>
            <th>Ação</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.CpfInfrator</td>
            <td>@item.NomeInfrator</td>
            <td>@item.ValorMulta</td>
            <td>@item.DataDeVencimento.ToShortDateString()</td>
            <td class="text-nowrap" width="10">
                <a class="btn btn-circle btn-info btn-sm" title="Editar" href="@Url.Action("Editar", new { id = item.Id })" data-togle="tooltip">
                    <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                </a>
                <a id="btn-excluir" class="btn btn-circle btn-danger btn-sm" title="Excluir" btn-sm" href="@Url.Action("Remover", new { id = item.Id })" data-togle="tooltip">
                    <i class="fa fa-close" aria-hidden="true"></i>
                </a>
            </td>
        </tr>
        }
    </tbody>
</table>